    -fdebug-prefix-map=/home/yasuhiro/Development/mozc-tsuki/mozc-tsuki=. $
    -fstack-protector-strong -Wformat -Werror=format-security -Wdate-time $
    -D_FORTIFY_SOURCE=2 -Wno-deprecated
arflags = 

build $
    obj/storage/louds/simple_succinct_bit_vector_index.simple_succinct_bit_vector_index.o: $
    cxx ../../storage/louds/simple_succinct_bit_vector_index.cc

build obj/storage/louds/libsimple_succinct_bit_vector_index.a: alink_thin $
    obj/storage/louds/simple_succinct_bit_vector_index.simple_succinct_bit_vector_index.o
