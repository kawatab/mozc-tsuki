rule $
    gen_separate_user_pos_data_for_chromeos_gen_separate_user_pos_data_for_chromeos_bbeed489eeb11633097095707d04e3d9
  command = cd ../../data_manager/chromeos; python $
      ../../dictionary/gen_user_pos_data.py $
      "--id_file=../../data/dictionary_chromeos/id.def" $
      "--special_pos_file=../../data/rules/special_pos.def" $
      "--user_pos_file=../../data/rules/user_pos.def" $
      "--cforms_file=../../data/rules/cforms.def" $
      "--output_token_array=../../out_linux/Debug/gen/data_manager/chromeos/user_pos_token_array.data" $
      "--output_string_array=../../out_linux/Debug/gen/data_manager/chromeos/user_pos_string_array.data" $
      "--output_pos_list=../../out_linux/Debug/gen/data_manager/chromeos/pos_list.data"
  description = ACTION(host) [chromeos] Generating user pos data.
  restat = 1

build gen/data_manager/chromeos/user_pos_token_array.data $
    gen/data_manager/chromeos/user_pos_string_array.data $
    gen/data_manager/chromeos/pos_list.data: $
    gen_separate_user_pos_data_for_chromeos_gen_separate_user_pos_data_for_chromeos_bbeed489eeb11633097095707d04e3d9 $
    ../../dictionary/gen_user_pos_data.py $
    ../../data/dictionary_chromeos/id.def ../../data/rules/special_pos.def $
    ../../data/rules/user_pos.def ../../data/rules/cforms.def

build $
    obj.host/data_manager/chromeos/gen_separate_user_pos_data_for_chromeos.actions_rules_copies.stamp: $
    stamp gen/data_manager/chromeos/user_pos_token_array.data $
    gen/data_manager/chromeos/user_pos_string_array.data $
    gen/data_manager/chromeos/pos_list.data

