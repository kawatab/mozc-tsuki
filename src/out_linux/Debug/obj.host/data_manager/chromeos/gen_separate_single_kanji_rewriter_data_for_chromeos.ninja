rule $
    gen_separate_single_kanji_rewriter_data_for_chromeos_gen_single_kanji_data_for_chromeos_d80abfe5a808bd1b9d8c1fb230003caf
  command = cd ../../data_manager/chromeos; python $
      ../../rewriter/gen_single_kanji_rewriter_data.py $
      "--single_kanji_file=../../data/single_kanji/single_kanji.tsv" $
      "--variant_file=../../data/single_kanji/variant_rule.txt" $
      "--output_single_kanji_token=../../out_linux/Debug/gen/data_manager/chromeos/single_kanji_token.data" $
      "--output_single_kanji_string=../../out_linux/Debug/gen/data_manager/chromeos/single_kanji_string.data" $
      "--output_variant_types=../../out_linux/Debug/gen/data_manager/chromeos/single_kanji_variant_type.data" $
      "--output_variant_tokens=../../out_linux/Debug/gen/data_manager/chromeos/single_kanji_variant_token.data" $
      "--output_variant_strings=../../out_linux/Debug/gen/data_manager/chromeos/single_kanji_variant_string.data"
  description = ACTION(host) [chromeos] Generating single kanji data
  restat = 1

build gen/data_manager/chromeos/single_kanji_string.data $
    gen/data_manager/chromeos/single_kanji_token.data $
    gen/data_manager/chromeos/single_kanji_variant_type.data $
    gen/data_manager/chromeos/single_kanji_variant_token.data $
    gen/data_manager/chromeos/single_kanji_variant_string.data: $
    gen_separate_single_kanji_rewriter_data_for_chromeos_gen_single_kanji_data_for_chromeos_d80abfe5a808bd1b9d8c1fb230003caf $
    ../../rewriter/gen_single_kanji_rewriter_data.py $
    ../../data/single_kanji/single_kanji.tsv $
    ../../data/single_kanji/variant_rule.txt || $
    gen_single_kanji_noun_prefix_data_main

rule $
    gen_separate_single_kanji_rewriter_data_for_chromeos_gen_noun_prefix_data_for_chromeos_d80abfe5a808bd1b9d8c1fb230003caf
  command = cd ../../data_manager/chromeos; $
      ../../out_linux/Debug/gen_single_kanji_noun_prefix_data_main $
      "--output_token_array=../../out_linux/Debug/gen/data_manager/chromeos/single_kanji_noun_prefix_token.data" $
      "--output_string_array=../../out_linux/Debug/gen/data_manager/chromeos/single_kanji_noun_prefix_string.data"
  description = ACTION(host) [chromeos] Generating noun prefix data
  restat = 1

build gen/data_manager/chromeos/single_kanji_noun_prefix_token.data $
    gen/data_manager/chromeos/single_kanji_noun_prefix_string.data: $
    gen_separate_single_kanji_rewriter_data_for_chromeos_gen_noun_prefix_data_for_chromeos_d80abfe5a808bd1b9d8c1fb230003caf $
    gen_single_kanji_noun_prefix_data_main || $
    gen_single_kanji_noun_prefix_data_main

build $
    obj.host/data_manager/chromeos/gen_separate_single_kanji_rewriter_data_for_chromeos.actions_rules_copies.stamp: $
    stamp gen/data_manager/chromeos/single_kanji_string.data $
    gen/data_manager/chromeos/single_kanji_token.data $
    gen/data_manager/chromeos/single_kanji_variant_type.data $
    gen/data_manager/chromeos/single_kanji_variant_token.data $
    gen/data_manager/chromeos/single_kanji_variant_string.data $
    gen/data_manager/chromeos/single_kanji_noun_prefix_token.data $
    gen/data_manager/chromeos/single_kanji_noun_prefix_string.data

