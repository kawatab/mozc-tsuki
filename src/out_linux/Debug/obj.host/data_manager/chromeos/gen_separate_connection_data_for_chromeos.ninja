rule $
    gen_separate_connection_data_for_chromeos_gen_separate_connection_data_for_chromeos_a0bc5a81e313be01f093bb241a1e70be
  command = cd ../../data_manager/chromeos; python $
      ../../data_manager/gen_connection_data.py --text_connection_file $
      ../../out_linux/Debug/gen/data_manager/chromeos/connection_single_column.txt $
      --id_file ../../data/dictionary_chromeos/id.def --special_pos_file $
      ../../data/rules/special_pos.def --binary_output_file $
      ../../out_linux/Debug/gen/data_manager/chromeos/connection.data $
      --target_compiler g++ --use_1byte_cost false
  description = ACTION(host) [chromeos] Generating $
      gen/data_manager/chromeos/connection.data
  restat = 1

build gen/data_manager/chromeos/connection.data: $
    gen_separate_connection_data_for_chromeos_gen_separate_connection_data_for_chromeos_a0bc5a81e313be01f093bb241a1e70be $
    gen/data_manager/chromeos/connection_single_column.txt $
    ../../data/dictionary_chromeos/id.def ../../data/rules/special_pos.def $
    || gen/data_manager/chromeos/connection_single_column.txt

ar = $ar_host
cc = $cc_host
cxx = $cxx_host
ld = $ld_host
ldxx = $ldxx_host
nm = $nm_host
readelf = $readelf_host
defines = -DOS_LINUX -DMOZC_BUILD -DCHANNEL_DEV -DENABLE_GTK_RENDERER -DDEBUG
includes = -I/home/yasuhiro/Development/mozc-tsuki/mozc-tsuki/src -Igen
cflags = -Wall -Wno-char-subscripts -Wno-sign-compare $
    -Wno-deprecated-declarations -Wwrite-strings $
    -Wno-unknown-warning-option -Wno-inconsistent-missing-override -fPIC $
    -fno-exceptions -fmessage-length=0 -fno-strict-aliasing -funsigned-char $
    -pipe -pthread -fno-omit-frame-pointer -fstack-protector $
    --param=ssp-buffer-size=4 -O0 -g
cflags_c = 
cflags_cc = -Wno-deprecated
arflags = 


