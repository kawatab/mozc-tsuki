rule $
    gen_separate_emoji_rewriter_data_for_chromeos_gen_separate_emoji_rewriter_data_for_chromeos_510a4b796ce082d7555ec195839b0339
  command = cd ../../data_manager/chromeos; python $
      ../../rewriter/gen_emoji_rewriter_data.py $
      "--input=../../data/emoji/emoji_data.tsv" $
      "--output_token_array=../../out_linux/Debug/gen/data_manager/chromeos/emoji_token.data" $
      "--output_string_array=../../out_linux/Debug/gen/data_manager/chromeos/emoji_string.data"
  description = ACTION(host) [chromeos] Generating emoji data
  restat = 1

build gen/data_manager/chromeos/emoji_token.data $
    gen/data_manager/chromeos/emoji_string.data: $
    gen_separate_emoji_rewriter_data_for_chromeos_gen_separate_emoji_rewriter_data_for_chromeos_510a4b796ce082d7555ec195839b0339 $
    ../../rewriter/gen_emoji_rewriter_data.py ../../data/emoji/emoji_data.tsv

build $
    obj.host/data_manager/chromeos/gen_separate_emoji_rewriter_data_for_chromeos.actions_rules_copies.stamp: $
    stamp gen/data_manager/chromeos/emoji_token.data $
    gen/data_manager/chromeos/emoji_string.data

