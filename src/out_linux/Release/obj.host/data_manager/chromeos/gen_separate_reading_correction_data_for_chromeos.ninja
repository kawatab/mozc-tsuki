rule $
    gen_separate_reading_correction_data_for_chromeos_gen_reading_correction_data_7728df15bd4991b7d4a96a3a831dfc2c
  command = cd ../../data_manager/chromeos; python $
      ../../rewriter/gen_reading_correction_data.py $
      "--input=../../data/dictionary_chromeos/reading_correction.tsv" $
      "--output_value_array=../../out_linux/Release/gen/data_manager/chromeos/reading_correction_value.data" $
      "--output_error_array=../../out_linux/Release/gen/data_manager/chromeos/reading_correction_error.data" $
      "--output_correction_array=../../out_linux/Release/gen/data_manager/chromeos/reading_correction_correction.data"
  description = ACTION(host) [chromeos] Generating $
      gen/data_manager/chromeos/reading_correction*
  restat = 1

build gen/data_manager/chromeos/reading_correction_value.data $
    gen/data_manager/chromeos/reading_correction_error.data $
    gen/data_manager/chromeos/reading_correction_correction.data: $
    gen_separate_reading_correction_data_for_chromeos_gen_reading_correction_data_7728df15bd4991b7d4a96a3a831dfc2c $
    ../../data/dictionary_chromeos/reading_correction.tsv

build $
    obj.host/data_manager/chromeos/gen_separate_reading_correction_data_for_chromeos.actions_rules_copies.stamp: $
    stamp gen/data_manager/chromeos/reading_correction_value.data $
    gen/data_manager/chromeos/reading_correction_error.data $
    gen/data_manager/chromeos/reading_correction_correction.data

