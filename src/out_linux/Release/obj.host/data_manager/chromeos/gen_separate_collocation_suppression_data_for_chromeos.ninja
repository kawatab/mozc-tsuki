rule $
    gen_separate_collocation_suppression_data_for_chromeos_gen_separate_collocation_suppression_data_fdb3bed243e113a392b14a13a750b7ad
  command = cd ../../data_manager/chromeos; $
      ../../out_linux/Release/gen_collocation_suppression_data_main $
      "--suppression_data=../../data/dictionary_chromeos/collocation_suppression.txt" $
      --binary_mode $
      "--output=../../out_linux/Release/gen/data_manager/chromeos/collocation_suppression_data.data"
  description = ACTION(host) [chromeos] Generating $
      gen/data_manager/chromeos/collocation_suppression_data.data
  restat = 1

build gen/data_manager/chromeos/collocation_suppression_data.data: $
    gen_separate_collocation_suppression_data_for_chromeos_gen_separate_collocation_suppression_data_fdb3bed243e113a392b14a13a750b7ad $
    gen_collocation_suppression_data_main $
    ../../data/dictionary_chromeos/collocation_suppression.txt || $
    gen_collocation_suppression_data_main

