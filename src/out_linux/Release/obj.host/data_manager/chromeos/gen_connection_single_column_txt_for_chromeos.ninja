rule $
    gen_connection_single_column_txt_for_chromeos_gen_connection_single_column_txt_for_chromeos_d7973030c0e36e3cbd87d3b121a0d9e9
  command = cd ../../data_manager/chromeos; python $
      ../../build_tools/zlib_util.py decompress $
      ../../data/dictionary_chromeos/connection.deflate $
      ../../out_linux/Release/gen/data_manager/chromeos/connection_single_column.txt
  description = ACTION(host) [chromeos] Decompressing $
      ../../data/dictionary_chromeos/connection.deflate
  restat = 1

build gen/data_manager/chromeos/connection_single_column.txt: $
    gen_connection_single_column_txt_for_chromeos_gen_connection_single_column_txt_for_chromeos_d7973030c0e36e3cbd87d3b121a0d9e9 $
    ../../data/dictionary_chromeos/connection.deflate

ar = $ar_host
cc = $cc_host
cxx = $cxx_host
ld = $ld_host
ldxx = $ldxx_host
nm = $nm_host
readelf = $readelf_host
defines = -DOS_LINUX -DMOZC_BUILD -DCHANNEL_DEV -DENABLE_GTK_RENDERER $
    -DNDEBUG -DQT_NO_DEBUG -DNO_LOGGING -DIGNORE_HELP_FLAG $
    -DIGNORE_INVALID_FLAG
includes = -I/home/yasuhiro/Development/mozc-tsuki/mozc-tsuki/src -Igen
cflags = -Wall -Wno-char-subscripts -Wno-sign-compare $
    -Wno-deprecated-declarations -Wwrite-strings $
    -Wno-unknown-warning-option -Wno-inconsistent-missing-override -fPIC $
    -fno-exceptions -fmessage-length=0 -fno-strict-aliasing -funsigned-char $
    -pipe -pthread -fno-omit-frame-pointer -fstack-protector $
    --param=ssp-buffer-size=4 -O2
cflags_c = 
