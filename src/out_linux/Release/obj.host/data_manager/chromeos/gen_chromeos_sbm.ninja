build $
    obj.host/data_manager/chromeos/gen_chromeos_sbm.actions_depends.stamp: $
    stamp obj.host/converter/libgen_segmenter_bitarray.a $
    gen/data_manager/chromeos/segmenter_inl.inc obj.host/base/libbase.a $
    obj.host/base/libbase_core.a obj.host/base/libclock.a $
    obj.host/base/libsingleton.a obj.host/base/libmutex.a $
    obj.host/base/libflags.a gen/base/character_set.inc $
    gen/base/version_def.h obj.host/base/libhash.a $
    obj.host/base/libstring_piece.a $
    obj.host/protocol/libsegmenter_data_proto.a $
    obj.host/protocol/genproto_segmenter_data_proto.actions_rules_copies.stamp

build $
    obj.host/data_manager/chromeos/gen_chromeos_sbm.compile_depends.stamp: $
    stamp $
    obj.host/protocol/genproto_segmenter_data_proto.actions_rules_copies.stamp $
    gen/data_manager/chromeos/segmenter_inl.inc $
    obj.host/base/base_core.compile_depends.stamp $
    gen/base/character_set.inc gen/base/version_def.h $
    obj.host/protocol/genproto_segmenter_data_proto.actions_rules_copies.stamp $
    obj.host/protocol/genproto_segmenter_data_proto.actions_rules_copies.stamp

ar = $ar_host
cc = $cc_host
cxx = $cxx_host
ld = $ld_host
ldxx = $ldxx_host
nm = $nm_host
readelf = $readelf_host
defines = -DOS_LINUX -DMOZC_BUILD -DCHANNEL_DEV -DENABLE_GTK_RENDERER $
    -DNDEBUG -DQT_NO_DEBUG -DNO_LOGGING -DIGNORE_HELP_FLAG $
    -DIGNORE_INVALID_FLAG
includes = -I/home/yasuhiro/Development/mozc-tsuki/mozc-tsuki/src -Igen $
    -Igen/proto_out
cflags = -Wall -Wno-char-subscripts -Wno-sign-compare $
    -Wno-deprecated-declarations -Wwrite-strings $
    -Wno-unknown-warning-option -Wno-inconsistent-missing-override -fPIC $
    -fno-exceptions -fmessage-length=0 -fno-strict-aliasing -funsigned-char $
    -pipe -pthread -fno-omit-frame-pointer -fstack-protector $
    --param=ssp-buffer-size=4 -O2
cflags_c = 
cflags_cc = -Wno-deprecated
arflags = 

build $
    obj.host/data_manager/chromeos/gen_chromeos_sbm.gen_chromeos_segmenter_bitarray_main.o: $
    cxx ../../data_manager/chromeos/gen_chromeos_segmenter_bitarray_main.cc $
    || obj.host/data_manager/chromeos/gen_chromeos_sbm.compile_depends.stamp

ldflags = -pthread
libs = -lprotobuf
build gen_chromeos_sbm: link $
    obj.host/data_manager/chromeos/gen_chromeos_sbm.gen_chromeos_segmenter_bitarray_main.o $
    obj.host/converter/libgen_segmenter_bitarray.a obj.host/base/libbase.a $
    obj.host/base/libbase_core.a obj.host/base/libclock.a $
    obj.host/base/libsingleton.a obj.host/base/libmutex.a $
    obj.host/base/libflags.a obj.host/base/libhash.a $
    obj.host/base/libstring_piece.a $
    obj.host/protocol/libsegmenter_data_proto.a | $
    gen/base/character_set.inc gen/base/version_def.h $
    gen/data_manager/chromeos/segmenter_inl.inc $
    obj.host/protocol/genproto_segmenter_data_proto.actions_rules_copies.stamp $
    || obj.host/data_manager/chromeos/gen_chromeos_sbm.actions_depends.stamp
  ld = $ldxx
