rule $
    gen_separate_symbol_rewriter_data_for_chromeos_gen_separate_symbol_rewriter_data_for_chromeos_72a6a9af39c4886364c79d57d5ccb0dc
  command = cd ../../data_manager/chromeos; $
      ../../out_linux/Release/gen_symbol_rewriter_dictionary_main $
      "--input=../../data/symbol/symbol.tsv" $
      "--user_pos_manager_data=../../out_linux/Release/gen/data_manager/chromeos/user_pos_manager.data" $
      "--sorting_table=../../data/rules/sorting_map.tsv" $
      "--ordering_rule=../../data/symbol/ordering_rule.txt" $
      "--output_token_array=../../out_linux/Release/gen/data_manager/chromeos/symbol_token.data" $
      "--output_string_array=../../out_linux/Release/gen/data_manager/chromeos/symbol_string.data"
  description = ACTION(host) [chromeos] Generating $
      gen/data_manager/chromeos/symbol*
  restat = 1

build gen/data_manager/chromeos/symbol_token.data $
    gen/data_manager/chromeos/symbol_string.data: $
    gen_separate_symbol_rewriter_data_for_chromeos_gen_separate_symbol_rewriter_data_for_chromeos_72a6a9af39c4886364c79d57d5ccb0dc $
    gen_symbol_rewriter_dictionary_main ../../data/symbol/symbol.tsv $
    ../../data/rules/sorting_map.tsv ../../data/symbol/ordering_rule.txt $
    gen/data_manager/chromeos/user_pos_manager.data || $
    gen_symbol_rewriter_dictionary_main

build $
    obj.host/data_manager/chromeos/gen_separate_symbol_rewriter_data_for_chromeos.actions_rules_copies.stamp: $
    stamp gen/data_manager/chromeos/symbol_token.data $
    gen/data_manager/chromeos/symbol_string.data

